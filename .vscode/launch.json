  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Tests",
      "program": "${workspaceRoot}/node_modules/@hapi/lab/bin/lab",
      "args": ["test"],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ],
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": null,
      "outputCapture": "std",
      "protocol": "inspector"
    }
  ]
}